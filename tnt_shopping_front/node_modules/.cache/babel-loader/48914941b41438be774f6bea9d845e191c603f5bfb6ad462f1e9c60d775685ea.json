             localStorage.setItem('user', JSON.stringify(res.data.data));\n              router.push('/home');\n            } else {\n              ElMessage.error(res.data.msg);\n            }\n          } catch (e) {\n            ElMessage.error('网络连接异常');\n          } finally {\n            loading.value = false;\n          }\n        }\n      });\n    };\n    const handleRegister = async () => {\n      if (!regFormRef.value) return;\n      await regFormRef.value.validate(async valid => {\n        if (valid) {\n          loading.value = true;\n          try {\n            const res = await proxy.$request.post('/user/register', {\n              username: regForm.username,\n              password: regForm.password\n            });\n            if (res.data.code === '200') {\n              ElMessage.success('注册成功，请登录');\n              toggleMode();\n            } else {\n              ElMessage.error(res.data.msg);\n            }\n          } catch (e) {\n            ElMessage.error('网络连接异常');\n          } finally {\n            loading.value = false;\n          }\n        }\n      });\n    };\n    const __returned__ = {\n      proxy,\n      router,\n      isRegister,\n      loading,\n      loginFormRef,\n      regFormRef,\n      loginForm,\n      regForm,\n      rules,\n      validatePass2,\n      regRules,\n      toggleMode,\n      handleLogin,\n      handleRegister,\n      ref,\n      reactive,\n      getCurrentInstance,\n      get useRouter() {\n        return useRouter;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get User() {\n        return User;\n      },\n      get Lock() {\n        return Lock;\n      },\n      get CircleCheck() {\n        return CircleCheck;\n      },\n      get ArrowRight() {\n        return ArrowRight;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","