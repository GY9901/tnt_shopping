     placeholder=\"PASSWORD / 密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <div class=\"btn-group\">\r\n            <el-button class=\"black-btn\" :loading=\"loading\" @click=\"handleLogin\">\r\n              登 录 <el-icon class=\"el-icon--right\"><ArrowRight /></el-icon>\r\n            </el-button>\r\n          </div>\r\n        </el-form>\r\n\r\n        <el-form\r\n            v-else\r\n            ref=\"regFormRef\"\r\n            :model=\"regForm\"\r\n            :rules=\"regRules\"\r\n            class=\"minimal-form\"\r\n        >\r\n          <el-form-item prop=\"username\">\r\n            <el-input\r\n                v-model=\"regForm.username\"\r\n                placeholder=\"SET USERNAME / 设置用户名\"\r\n                :prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input\r\n                v-model=\"regForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"PASSWORD / 密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"confirmPass\">\r\n            <el-input\r\n                v-model=\"regForm.confirmPass\"\r\n                type=\"password\"\r\n                placeholder=\"CONFIRM / 确认密码\"\r\n                show-password\r\n                :prefix-icon=\"CircleCheck\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <div class=\"btn-group\">\r\n            <el-button class=\"black-btn\" :loading=\"loading\" @click=\"handleRegister\">\r\n              注 册 <el-icon class=\"el-icon--right\"><ArrowRight /></el-icon>\r\n            </el-button>\r\n          </div>\r\n        </el-form>\r\n\r\n        <div class=\"footer-switch\">\r\n          <span v-if=\"!isRegister\">\r\n            还没有账号? <a @click=\"toggleMode\">立即加入</a>\r\n          </span>\r\n          <span v-else>\r\n            已有账号? <a @click=\"toggleMode\">直接登录</a>\r\n          </span>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, getCurrentInstance } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { User, Lock, CircleCheck, ArrowRight } from '@element-plus/icons-vue'\r\n\r\nconst { proxy } = getCurrentInstance()\r\nconst router = useRouter()\r\n\r\nconst isRegister = ref(false)\r\nconst loading = ref(false)\r\nconst loginFormRef = ref(null)\r\nconst regFormRef = ref(null)\r\n\r\nconst loginForm = reactive({ username: '', password: '' })\r\nconst regForm = reactive({ username: '', password: '', confirmPass: '' })\r\n\r\nconst rules = {\r\n  username: [{ required: true, message: '请输入用户名', trigger: 'blur' }],\r\n  password: [{ required: true, message: '请输入密码', trigger: 'blur' }]\r\n}\r\n\r\nconst validatePass2 = (rule, value, callback) => {\r\n  if (value === '') callback(new Error('请再次输入密码'))\r\n  else if (value !== regForm.password) callback(new Error('两次输入密码不一致!'))\r\n  else callback()\r\n}\r\n\r\nconst regRules = {\r\n  ...rules,\r\n  confirmPass: [{ validator: validatePass2, trigger: 'blur' }]\r\n}\r\n\r\nconst toggleMode = () => {\r\n  isRegister.value = !isRegister.value\r\n  loginFormRef.value?.resetFields()\r\n  regFormRef.value?.resetFields()\r\n}\r\n\r\nconst handleLogin = async () => {\r\n  if (!loginFormRef.value) return\r\n  await loginFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      loading.value = true\r\n      try {\r\n        const res = await proxy.$request.post('/user/login', loginForm)\r\n        if (res.data.code === '200') {\r\n          ElMessage.success('欢迎回来')\r\n          localStorage.setItem('user', JSON.stringify(res.data.data))\r\n          router.push('/home')\r\n        } else {\r\n          ElMessage.error(res.data.msg)\r\n        }\r\n      } catch (e) {\r\n        ElMessage.error('网络连接异常')\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n  })\r\n}\r\n\r\nconst handleRegister = async () => {\r\n  if (!regFormRef.value) return\r\n  await regFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      loading.value = true\r\n      try {\r\n        const res = await proxy.$request.post('/user/register', {\r\n          username: regForm.username,\r\n          password: regForm.password\r\n        })\r\n        if (res.data.code === '200') {\r\n          ElMessage.success('注册成功，请登录')\r\n          toggleMode()\r\n        } else {\r\n          ElMessage.error(res.data.msg)\r\n        }\r\n      } catch (e) {\r\n        ElMessage.error('网络连接异常')\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n  })\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 颜色变量：米白 + 纯黑 + 亮黄 */\r\n:root {\r\n  --bg-color: #FDFBF0;       /* 浅奶黄/米白背景，非常柔和 */\r\n  --card-bg: #FFFFFF;        /* 卡片纯白 */\r\n  --accent-black: #000000;   /* 纯黑 */\r\n  --accent-yellow: #FFD700;  /* 纯正明亮的黄色 */\r\n  --text-gray: #888888;\r\n}\r\n\r\n.login-container {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background-color: #FDFBF0; /* 整体背景淡奶色 */\r\n  font-family: 'Helvetica Neue', Helvetica, 'PingFang SC', sans-serif;\r\n}\r\n\r\n.main-card {\r\n  width: 900px;\r\n  height: 550px;\r\n  background: white;\r\n  box-shadow: 0 20px 60px rgba(0, 0, 0, 0.08); /* 极简阴影 */\r\n  display: flex;\r\n  overflow: hidden;\r\n  border-radius: 0; /* 直角设计更具时尚感 */\r\n}\r\n\r\n/* 左侧视觉区 */\r\n.visual-side {\r\n  width: 45%;\r\n  background-color: #000000;\r\n  color: #FFD700;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  padding: 60px;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.brand-big {\r\n  font-size: 80px;\r\n  font-weight: 900;\r\n  line-height: 0.9;\r\n  margin: 0;\r\n  letter-spacing: -2px;\