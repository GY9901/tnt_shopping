[!$setup.loading ? (_openBlock(), _createElementBlock(\"span\", _hoisted_7, \"注册账号\")) : (_openBlock(), _createElementBlock(\"span\", _hoisted_8, \"处理中...\"))])]),\n    _: 1 /* STABLE */\n  }, 8 /* PROPS */, [\"model\"])], 2112 /* STABLE_FRAGMENT, DEV_ROOT_FRAGMENT */)), _createCommentVNode(\" 底部切换 \"), _createElementVNode(\"div\", _hoisted_9, [_createElementVNode(\"div\", {\n    class: \"switch-text\",\n    onClick: $setup.toggleMode\n  }, _toDisplayString($setup.isRegister ? '已有账号？去登录' : '没有账号？去注册'), 1 /* TEXT */)])])])]);\n}","map":{"version":3,"names":["class","_createElementBlock","_hoisted_1","_createElementVNode","_hoisted_2","_createCommentVNode","_hoisted_3","_hoisted_4","_toDisplayString","$setup","isRegister","_createBlock","_component_el_form","ref","model","loginForm","rules","size","_createVNode","_component_el_form_item","prop","_component_el_input","username","$event","placeholder","User","password","type","Lock","onClick","_withModifiers","handleLogin","loading","_hoisted_5","_hoisted_6","_Fragment","key","regForm","regRules","confirmPass","CircleCheck","handleRegister","_hoisted_7","_hoisted_8","_hoisted_9","toggleMode"],"sources":["E:\\xianyu\\tnt\\tnt_shopping_front\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-wrapper\">\r\n    <div class=\"auth-container\">\r\n\r\n      <!-- 左侧：品牌视觉区 (黑色背景，黄色点缀) -->\r\n      <div class=\"visual-panel\">\r\n        <div class=\"brand-content\">\r\n          <div class=\"logo-circle\">TNT</div>\r\n          <h1 class=\"brand-title\">TEENS IN TIMES</h1>\r\n          <p class=\"brand-subtitle\">OFFICIAL MERCHANDISE STORE</p>\r\n\r\n          <div class=\"yellow-box-decor\"></div>\r\n\r\n          <div class=\"slogan-vertical\">\r\n            <span>破</span>\r\n            <span>天</span>\r\n            <span>下</span>\r\n            <span class=\"dot\">·</span>\r\n            <span>定</span>\r\n            <span>风</span>\r\n            <span>云</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 右侧：交互表单区 (浅黄背景，黑色控件) -->\r\n      <div class=\"form-panel\">\r\n        <div class=\"form-header\">\r\n          <h2>{{ isRegister ? '成为会员' : '欢迎回来' }}</h2>\r\n          <p>{{ isRegister ? '注册账号开启购物之旅' : '登录您的账号' }}</p>\r\n        </div>\r\n\r\n        <!-- 登录表单 -->\r\n        <el-form\r\n            v-if=\"!isRegister\"\r\n            ref=\"loginFormRef\"\r\n            :model=\"loginForm\"\r\n            :rules=\"rules\"\r\n            class=\"custom-form\"\r\n            size=\"large\"\r\n        >\r\n          <el-form-item prop=\"username\">\r\n            <el-input\r\n                v-model=\"loginForm.username\"\r\n                placeholder=\"请输入用户名\"\r\n                :prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input\r\n                v-model=\"loginForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"请输入密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <button class=\"tnt-btn\" @click.prevent=\"handleLogin\">\r\n            <span v-if=\"!loading\">立即登录</span>\r\n            <span v-else>登录中...</span>\r\n          </button>\r\n        </el-form>\r\n\r\n        <!-- 注册表单 -->\r\n        <el-form\r\n            v-else\r\n            ref=\"regFormRef\"\r\n            :model=\"regForm\"\r\n            :rules=\"regRules\"\r\n            class=\"custom-form\"\r\n            size=\"large\"\r\n        >\r\n          <el-form-item prop=\"username\">\r\n            <el-input\r\n                v-model=\"regForm.username\"\r\n                placeholder=\"设置用户名\"\r\n                :prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input\r\n                v-model=\"regForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"设置密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"confirmPass\">\r\n            <el-input\r\n                v-model=\"regForm.confirmPass\"\r\n                type=\"password\"\r\n                placeholder=\"确认密码\"\r\n                show-password\r\n                :prefix-icon=\"CircleCheck\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <button class=\"tnt-btn outline\" @click.prevent=\"handleRegister\">\r\n            <span v-if=\"!loading\">注册账号</span>\r\n            <span v-else>处理中...</span>\r\n          </button>\r\n        </el-form>\r\n\r\n        <!-- 底部切换 -->\r\n        <div class=\"panel-footer\">\r\n          <div class=\"switch-text\" @click=\"toggleMode\">\r\n            {{ isRegister ? '已有账号？去登录' : '没有账号？去注册' }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, getCurrentInstance } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { User, Lock, CircleCheck } from '@element-plus/icons-vue'\r\n\r\nconst { proxy } = getCurrentInstance()\r\nconst router = useRouter()\r\n\r\nconst isRegister = ref(false)\r\nconst loading = ref(false)\r\nconst loginFormRef = ref(null)\r\nconst regFormRef = ref(null)\r\n\r\nconst loginForm = reactive({ username: '', password: '' })\r\nconst regForm = reactive({ username: '', password: '', confirmPass: '' })\r\n\r\n// 规则配置\r\nconst rules = {\r\n  username: [{ required: true, message: '请输入用户名', trigger: 'blur' }],\r\n  password: [{ required: true, message: '请输入密码', trigger: 'blur' }]\r\n}\r\n\r\nconst validatePass2 = (rule, value, callback) => {\r\n  if (value === '') callback(new Error('请再次输入密码'))\r\n  else if (value !== regForm.password) callback(new Error('两次密码不一致'))\r\n  else callback()\r\n}\r\n\r\nconst regRules = {\r\n  ...rules,\r\n  confirmPass: [{ validator: validatePass2, trigger: 'blur' }]\r\n}\r\n\r\n// 切换模式\r\nconst toggleMode = () => {\r\n  isRegister.value = !isRegister.value\r\n  loginFormRef.value?.resetFields()\r\n  regFormRef.value?.resetFields()\r\n}\r\n\r\n// 登录请求\r\nconst handleLogin = async () => {\r\n  if (!loginFormRef.value) return\r\n\r\n  await loginFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      loading.value = true\r\n      try {\r\n        const res = await proxy.$request.post('/user/login', loginForm)\r\n\r\n        if (res.data.code === '200') {\r\n          ElMessage.success('登录成功')\r\n          localStorage.setItem('user', JSON.stringify(res.data.data))\r\n\r\n          // 延时跳转体验更好\r\n          setTimeout(() => {\r\n            router.push('/home')\r\n          }, 500)\r\n        } else {\r\n          ElMessage.error(res.data.msg || '登录失败')\r\n        }\r\n      } catch (error) {\r\n        console.error(error)\r\n        ElMessage.error('服务器连接异常')\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n  })\r\n}\r\n\r\n// 注册请求\r\nconst handleRegister = async () => {\r\n  if (!regFormRef.value) return\r\n  await regFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      loading.value = true\r\n      try {\r\n        const res = await proxy.$request.post('/user/register', {\r\n          username: regForm.username,\r\n          password: regForm.password\r\n        })\r\n        if (res.data.code === '200') {\r\n          ElMessage.success('注册成功，请登录')\r\n          toggleMode()\r\n        } else {\r\n          ElMessage.error(res.data.msg)\r\n        }\r\n      } catch (error) {\r\n        ElMessage.error('服务器连接异常')\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n  })\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url('https://fonts.googleapis.com/css2?family=Oswald:wght@400;700&display=swap');\r\n\r\n/* 配色系统：浅奶黄 + 纯黑 */\r\n:root {\r\n  --bg-cream: #FFFDF0;       /* 页面大背景：极浅的奶黄 */\r\n  --panel-cream: #FFFCxE;    /* 面板背景：稍深一点的黄 */\r\n  --main-black: #1A1A1A;     /* 主黑色 */\r\n  --tnt-yellow: #FAD02C;     /* 亮黄点缀 */\r\n}\r\n\r\n.page-wrapper {\r\n  min-height: 100vh;\r\n  background-color: #FFFDF0; /* 全局浅黄背景 */\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-family: 'Helvetica Neue', Helvetica, 'PingFang SC', sans-serif;\r\n}\r\n\r\n.auth-container {\r\n  width: 900px;\r\n  height: 550px;\r\n  background: white;\r\n  display: flex;\r\n  box-shadow: 0 20px 50px rgba(26, 26, 26, 0.15); /* 黑色阴影 */\r\n  border: 4px solid var(--main-black); /* 黑色粗边框，增加漫画感/硬朗感 */\r\n}\r\n\r\n/* --- 左侧视觉区 --- */\r\n.visual-panel {\r\n  width: 45%;\r\n  background-color: var(--main-black);\r\n  color: var(--tnt-yellow);\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: relative;\r\n  overflow: hidden;\r\n  padding: 40px;\r\n  text-align: center;\r\n}\r\n\r\n.brand-content {\r\n  z-index: 2;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.logo-circle {\r\n  width: 80px;\r\n  height: 80px;\r\n  border: 4px solid var(--tnt-yellow);\r\n  border-radius: 50%;\r\n  font-family: 'Oswald', sans-serif;\r\n  font-size: 32px;\r\n  font-weight: bold;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.brand-title {\r\n  font-family: 'Oswald', sans-serif;\r\n  font-size: 36px;\r\n  letter-spacing: 2px;\r\n  margin: 0;\r\n  color: white;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.brand-subtitle {\r\n  font-size: 12px;\r\n  color: #888;\r\n  letter-spacing: 3px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.yellow-box-decor {\r\n  width: 40px;\r\n  height: 8px;\r\n  background-color: var(--tnt-yellow);\r\n  margin: 30px 0;\r\n}\r\n\r\n.slogan-vertical {\r\n  display: flex;\r\n  gap: 8px;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  color: rgba(255, 255, 255, 0.8);\r\n}\r\n\r\n/* --- 右侧表单区 --- */\r\n.form-panel {\r\n  flex: 1;\r\n  background-color: #FFFEF5; /* 极浅的黄白 */\r\n  padding: 60px 80px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  position: relative;\r\n}\r\n\r\n.form-header {\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.form-header h2 {\r\n  font-size: 28px;\r\n  color: var(--main-black);\r\n  margin: 0 0 10px 0;\r\n  font-weight: 800;\r\n}\r\n\r\n.form-header p {\r\n  color: #666;\r\n  font-size: 14px;\r\n}\r\n\r\n/* 自定义 Input 样式：黑边框 */\r\n.custom-form :deep(.el-input__wrapper) {\r\n  background-color: transparent;\r\n  border: 2px solid #E0E0E0;\r\n  border-radius: 4px;\r\n  box-shadow: none !important;\r\n  transition: all 0.3s;\r\n  height: 44px;\r\n}\r\n\r\n.custom-form :deep(.el-input__wrapper.is-focus) {\r\n  border-color: var(--main-black); /* 聚焦变黑 */\r\n  background-color: white;\r\n}\r\n\r\n.custom-form :deep(.el-input__inner) {\r\n  color: var(--main-black);\r\n  font-weight: 500;\r\n}\r\n\r\n/* 按钮样式：黑底黄字 */\r\n.tnt-btn {\r\n  width: 100%;\r\n  height: 50px;\r\n  background-color: var(--main-black);\r\n  color: var(--tnt-yellow);\r\n  border: 2px solid var(--main-black);\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  margin-top: 20px;\r\n  transition: all 0.3s;\r\n  text-transform: uppercase;\r\n  letter-spacing: 1px;\r\n}\r\n\r\n.tnt-btn:hover {\r\n  background-color: var(--tnt-yellow);\r\n  color: var(--main-black);\r\n  border-color: var(--main-black);\r\n  box-shadow: 4px 4px 0 rgba(0,0,0,0.2);\r\n}\r\n\r\n/* 注册按钮样式：线框风格 */\r\n.tnt-btn.outline {\r\n  background-color: transparent;\r\n  color: var(--main-black);\r\n  border: 2px solid var(--main-black);\r\n}\r\n\r\n.tnt-btn.outline:hover {\r\n  background-color: var(--main-black);\r\n  color: white;\r\n}\r\n\r\n.panel-footer {\r\n  margin-top: 30px;\r\n  text-align: center;\r\n}\r\n\r\n.switch-text {\r\n  display: inline-block;\r\n  font-size: 14px;\r\n  color: #666;\r\n  cursor: pointer;\r\n  