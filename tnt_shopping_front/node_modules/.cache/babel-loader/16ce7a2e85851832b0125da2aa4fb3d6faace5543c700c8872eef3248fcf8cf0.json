{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport { ref, reactive, getCurrentInstance } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { User, Lock, CircleCheck, ArrowRightBold } from '@element-plus/icons-vue';\nexport default {\n  __name: 'Login',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const {\n      proxy\n    } = getCurrentInstance();\n    const router = useRouter(); // 必须使用这个 router 实例进行跳转\n\n    const isRegister = ref(false);\n    const loginFormRef = ref(null);\n    const regFormRef = ref(null);\n    const loginForm = reactive({\n      username: '',\n      password: ''\n    });\n    const regForm = reactive({\n      username: '',\n      password: '',\n      confirmPass: ''\n    });\n    const rules = {\n      username: [{\n        required: true,\n        message: '请输入账号',\n        trigger: 'blur'\n      }],\n      password: [{\n        required: true,\n        message: '请输入密码',\n        trigger: 'blur'\n      }]\n    };\n    const validatePass2 = (rule, value, callback) => {\n      if (value === '') callback(new Error('请再次输入密码'));else if (value !== regForm.password) callback(new Error('两次密码不一致'));else callback();\n    };\n    const regRules = {\n      ...rules,\n      confirmPass: [{\n        validator: validatePass2,\n        trigger: 'blur'\n      }]\n    };\n    const toggleMode = () => {\n      isRegister.value = !isRegister.value;\n      loginFormRef.value?.resetFields();\n      regFormRef.value?.resetFields();\n    };\n    const handleLogin = async () => {\n      if (!loginFormRef.value) return;\n      await loginFormRef.value.validate(async valid => {\n        if (valid) {\n          try {\n            const res = await proxy.$request.post('/user/login', loginForm);\n            console.log(\"登录响应:\", res.data); // 调试日志\n\n            if (res.data.code === '200') {\n              ElMessage.success({\n                message: '登录成功，正在跳转...',\n                type: 'success',\n                duration: 1000\n              });\n              // 存储用户信息\n              localStorage.setItem('user', JSON.stringify(res.data.data));\n\n              // 核心跳转逻辑：使用 replace 防止用户点后退回到登录页\n              setTimeout(() => {\n                router.replace('/home').catch(err => {\n                  console.error(\"跳转失败:\", err);\n                });\n              }, 500);\n            } else {\n              ElMessage.error(res.data.msg || '登录失败');\n            }\n          } catch (error) {\n            console.error(error);\n            ElMessage.error('服务器连接异常');\n          }\n        }\n      });\n    };\n    const handleRegister = async () => {\n      if (!regFormRef.value) return;\n      await regFormRef.value.validate(async valid => {\n        if (valid) {\n          try {\n            const res = await proxy.$request.post('/user/register', {\n              username: regForm.username,\n              password: regForm.password\n            });\n            if (res.data.code === '200') {\n              ElMessage.success('注册成功，请登录');\n              toggleMode();\n            } else {\n              ElMessage.error(res.data.msg);\n            }\n          } catch (error) {\n            ElMessage.error('服务器连接异常');\n          }\n        }\n      });\n    };\n    const __returned__ = {\n      proxy,\n      router,\n      isRegister,\n      loginFormRef,\n      regFormRef,\n      loginForm,\n      regForm,\n      rules,\n      validatePass2,\n      regRules,\n      toggleMode,\n      handleLogin,\n      handleRegister,\n      ref,\n      reactive,\n      getCurrentInstance,\n      get useRouter() {\n        return useRouter;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get User() {\n        return User;\n      },\n      get Lock() {\n        return Lock;\n      },\n      get CircleCheck() {\n        return CircleCheck;\n      },\n      get ArrowRightBold() {\n        return ArrowRightBold;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","getCurrentInstance","useRouter","ElMessage","User","Lock","CircleCheck","ArrowRightBold","proxy","router","isRegister","loginFormRef","regFormRef","loginForm","username","password","regForm","confirmPass","rules","required","message","trigger","validatePass2","rule","value","callback","Error","regRules","validator","toggleMode","resetFields","handleLogin","validate","valid","res","$request","post","console","log","data","code","success","type","duration","localStorage","setItem","JSON","stringify","setTimeout","replace","catch","err","error","msg","handleRegister"],"sources":["E:/xianyu/tnt/tnt_shopping_front/src/views/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"bg-text\">TNT SPACE</div>\r\n\r\n    <div class=\"login-card\">\r\n      <div class=\"card-left\">\r\n        <div class=\"visual-content\">\r\n          <div class=\"logo-box\">TNT</div>\r\n          <h2 class=\"slogan-en\">TEENS IN TIMES</h2>\r\n          <div class=\"yellow-bar\"></div>\r\n          <p class=\"slogan-cn\">破天下 · 定风云</p>\r\n          <div class=\"copyright\">© 2025 OFFICIAL MERCH</div>\r\n        </div>\r\n        <div class=\"light-effect\"></div>\r\n      </div>\r\n\r\n      <div class=\"card-right\">\r\n        <div class=\"header\">\r\n          <h3>{{ isRegister ? 'JOIN US' : 'SIGN IN' }}</h3>\r\n          <span class=\"dot\"></span>\r\n        </div>\r\n\r\n        <el-form\r\n            v-if=\"!isRegister\"\r\n            ref=\"loginFormRef\"\r\n            :model=\"loginForm\"\r\n            :rules=\"rules\"\r\n            class=\"street-form\"\r\n            hide-required-asterisk\r\n        >\r\n          <el-form-item prop=\"username\">\r\n            <div class=\"input-label\">ACCOUNT</div>\r\n            <el-input\r\n                v-model=\"loginForm.username\"\r\n                placeholder=\"请输入用户名\"\r\n                :prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <div class=\"input-label\">PASSWORD</div>\r\n            <el-input\r\n                v-model=\"loginForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"请输入密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <button class=\"cyber-btn\" @click.prevent=\"handleLogin\">\r\n            <span class=\"btn-content\">\r\n              立即登录 <el-icon><ArrowRightBold /></el-icon>\r\n            </span>\r\n            <div class=\"btn-glitch\"></div>\r\n          </button>\r\n        </el-form>\r\n\r\n        <el-form\r\n            v-else\r\n            ref=\"regFormRef\"\r\n            :model=\"regForm\"\r\n            :rules=\"regRules\"\r\n            class=\"street-form\"\r\n            hide-required-asterisk\r\n        >\r\n          <el-form-item prop=\"username\">\r\n            <div class=\"input-label\">NEW ACCOUNT</div>\r\n            <el-input\r\n                v-model=\"regForm.username\"\r\n                placeholder=\"设置用户名\"\r\n                :prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <div class=\"input-label\">PASSWORD</div>\r\n            <el-input\r\n                v-model=\"regForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"设置密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"confirmPass\">\r\n            <div class=\"input-label\">CONFIRM</div>\r\n            <el-input\r\n                v-model=\"regForm.confirmPass\"\r\n                type=\"password\"\r\n                placeholder=\"确认密码\"\r\n                show-password\r\n                :prefix-icon=\"CircleCheck\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <button class=\"cyber-btn register-mode\" @click.prevent=\"handleRegister\">\r\n            <span class=\"btn-content\">\r\n              注册会员 <el-icon><ArrowRightBold /></el-icon>\r\n            </span>\r\n          </button>\r\n        </el-form>\r\n\r\n        <div class=\"footer-link\">\r\n          <span @click=\"toggleMode\">\r\n            {{ isRegister ? '← 返回登录' : '注册新账号 →' }}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, getCurrentInstance } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { User, Lock, CircleCheck, ArrowRightBold } from '@element-plus/icons-vue'\r\n\r\nconst { proxy } = getCurrentInstance()\r\nconst router = useRouter() // 必须使用这个 router 实例进行跳转\r\n\r\nconst isRegister = ref(false)\r\nconst loginFormRef = ref(null)\r\nconst regFormRef = ref(null)\r\n\r\nconst loginForm = reactive({ username: '', password: '' })\r\nconst regForm = reactive({ username: '', password: '', confirmPass: '' })\r\n\r\nconst rules = {\r\n  username: [{ required: true, message: '请输入账号', trigger: 'blur' }],\r\n  password: [{ required: true, message: '请输入密码', trigger: 'blur' }]\r\n}\r\n\r\nconst validatePass2 = (rule, value, callback) => {\r\n  if (value === '') callback(new Error('请再次输入密码'))\r\n  else if (value !== regForm.password) callback(new Error('两次密码不一致'))\r\n  else callback()\r\n}\r\n\r\nconst regRules = {\r\n  ...rules,\r\n  confirmPass: [{ validator: validatePass2, trigger: 'blur' }]\r\n}\r\n\r\nconst toggleMode = () => {\r\n  isRegister.value = !isRegister.value\r\n  loginFormRef.value?.resetFields()\r\n  regFormRef.value?.resetFields()\r\n}\r\n\r\nconst handleLogin = async () => {\r\n  if (!loginFormRef.value) return\r\n\r\n  await loginFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      try {\r\n        const res = await proxy.$request.post('/user/login', loginForm)\r\n        console.log(\"登录响应:\", res.data); // 调试日志\r\n\r\n        if (res.data.code === '200') {\r\n          ElMessage.success({\r\n            message: '登录成功，正在跳转...',\r\n            type: 'success',\r\n            duration: 1000\r\n          })\r\n          // 存储用户信息\r\n          localStorage.setItem('user', JSON.stringify(res.data.data))\r\n\r\n          // 核心跳转逻辑：使用 replace 防止用户点后退回到登录页\r\n          setTimeout(() => {\r\n            router.replace('/home').catch(err => {\r\n              console.error(\"跳转失败:\", err)\r\n            })\r\n          }, 500)\r\n\r\n        } else {\r\n          ElMessage.error(res.data.msg || '登录失败')\r\n        }\r\n      } catch (error) {\r\n        console.error(error)\r\n        ElMessage.error('服务器连接异常')\r\n      }\r\n    }\r\n  })\r\n}\r\n\r\nconst handleRegister = async () => {\r\n  if (!regFormRef.value) return\r\n  await regFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      try {\r\n        const res = await proxy.$request.post('/user/register', {\r\n          username: regForm.username,\r\n          password: regForm.password\r\n        })\r\n        if (res.data.code === '200') {\r\n          ElMessage.success('注册成功，请登录')\r\n          toggleMode()\r\n        } else {\r\n          ElMessage.error(res.data.msg)\r\n        }\r\n      } catch (error) {\r\n        ElMessage.error('服务器连接异常')\r\n      }\r\n    }\r\n  })\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 引入谷歌字体增加设计感 (可选) */\r\n@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700;900&display=swap');\r\n\r\n:root {\r\n  --main-black: #0F0F0F;\r\n  --main-yellow: #FFEA00; /* 高亮电光黄 */\r\n  --text-grey: #666;\r\n  --bg-color: #F0F0F0;\r\n}\r\n\r\n.login-container {\r\n  height: 100vh;\r\n  width: 100vw;\r\n  background-color: var(--bg-color);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: relative;\r\n  overflow: hidden;\r\n  font-family: 'Montserrat', sans-serif;\r\n}\r\n\r\n/* 背景巨大文字装饰 */\r\n.bg-text {\r\n  position: absolute;\r\n  font-size: 20vw;\r\n  font-weight: 900;\r\n  color: rgba(0, 0, 0, 0.03);\r\n  z-index: 0;\r\n  white-space: nowrap;\r\n  pointer-events: none;\r\n  transform: rotate(-10deg);\r\n}\r\n\r\n.login-card {\r\n  width: 900px;\r\n  height: 520px;\r\n  background: #fff;\r\n  z-index: 1;\r\n  display: flex;\r\n  box-shadow: 0 30px 60px rgba(0,0,0,0.15); /* 强阴影 */\r\n}\r\n\r\n/* 左侧设计 */\r\n.card-left {\r\n  width: 45%;\r\n  background-color: var(--main-black);\r\n  position: relative;\r\n  padding: 50px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  overflow: hidden;\r\n  color: white;\r\n}\r\n\r\n.visual-content {\r\n  z-index: 2;\r\n}\r\n\r\n.logo-box {\r\n  font-size: 60px;\r\n  font-weight: 900;\r\n  letter-spacing: -3px;\r\n  line-height: 1;\r\n  color: var(--main-yellow);\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.slogan-en {\r\n  font-size: 24px;\r\n  font-weight: 700;\r\n  letter-spacing: 2px;\r\n  margin: 0;\r\n}\r\n\r\n.yellow-bar {\r\n  width: 60px;\r\n  height: 6px;\r\n  background: var(--main-yellow);\r\n  margin: 20px 0;\r\n}\r\n\r\n.slogan-cn {\r\n  font-size: 16px;\r\n  letter-spacing: 5px;\r\n  opacity: 0.8;\r\n}\r\n\r\n.copyright {\r\n  position: absolute;\r\n  bottom: 30px;\r\n  font-size: 10px;\r\n  opacity: 0.3;\r\n}\r\n\r\n/* 流光效果 */\r\n.light-effect {\r\n  position: absolute;\r\n  top: -50%;\r\n  left: -50%;\r\n  width: 200%;\r\n  height: 200%;\r\n  background: radial-gradient(circle, rgba(255, 234, 0, 0.1) 0%, transparent 60%);\r\n  animation: pulse 5s infinite;\r\n}\r\n\r\n@keyframes pulse {\r\n  0% { transform: scale(1); opacity: 0.5; }\r\n  50% { transform: scale(1.1); opacity: 0.8; }\r\n  100% { transform: scale(1); opacity: 0.5; }\r\n}\r\n\r\n/* 右侧设计 */\r\n.card-right {\r\n  flex: 1;\r\n  paddin