{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport { ref, reactive, getCurrentInstance } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { User, Lock, CircleCheck } from '@element-plus/icons-vue';\nexport default {\n  __name: 'Login',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const {\n      proxy\n    } = getCurrentInstance();\n    const router = useRouter();\n    const isRegister = ref(false);\n    const loading = ref(false);\n    const loginFormRef = ref(null);\n    const regFormRef = ref(null);\n    const loginForm = reactive({\n      username: '',\n      password: ''\n    });\n    const regForm = reactive({\n      username: '',\n      password: '',\n      confirmPass: ''\n    });\n\n    // 校验规则\n    const rules = {\n      username: [{\n        required: true,\n        message: 'Please input username',\n        trigger: 'blur'\n      }],\n      password: [{\n        required: true,\n        message: 'Please input password',\n        trigger: 'blur'\n      }]\n    };\n    const validatePass2 = (rule, value, callback) => {\n      if (value === '') callback(new Error('Please confirm password'));else if (value !== regForm.password) callback(new Error('Password mismatch'));else callback();\n    };\n    const regRules = {\n      ...rules,\n      confirmPass: [{\n        validator: validatePass2,\n        trigger: 'blur'\n      }]\n    };\n\n    // 切换逻辑\n    const toggleMode = () => {\n      isRegister.value = !isRegister.value;\n      loginFormRef.value?.resetFields();\n      regFormRef.value?.resetFields();\n    };\n\n    // 登录\n    const handleLogin = async () => {\n      if (!loginFormRef.value) return;\n      await loginFormRef.value.validate(async valid => {\n        if (valid) {\n          loading.value = true;\n          try {\n            const res = await proxy.$request.post('/user/login', loginForm);\n            if (res.data.code === '200') {\n              ElMessage.success({\n                message: '登录成功 / Welcome',\n                type: 'success',\n                duration: 1500\n              });\n              localStorage.setItem('user', JSON.stringify(res.data.data));\n              setTimeout(() => {\n                router.replace('/home');\n              }, 500);\n            } else {\n              ElMessage.error(res.data.msg || 'Login failed');\n            }\n          } catch (error) {\n            ElMessage.error('Server Error');\n          } finally {\n            loading.value = false;\n          }\n        }\n      });\n    };\n\n    // 注册\n    const handleRegister = async () => {\n      if (!regFormRef.value) return;\n      await regFormRef.value.validate(async valid => {\n        if (valid) {\n          loading.value = true;\n          try {\n            const res = await proxy.$request.post('/user/register', {\n              username: regForm.username,\n              password: regForm.password\n            });\n            if (res.data.code === '200') {\n              ElMessage.success('Register Success');\n              toggleMode();\n            } else {\n              ElMessage.error(res.data.msg);\n            }\n          } catch (error) {\n            ElMessage.error('Server Error');\n          } finally {\n            loading.value = false;\n          }\n        }\n      });\n    };\n    const __returned__ = {\n      proxy,\n      router,\n      isRegister,\n      loading,\n      loginFormRef,\n      regFormRef,\n      loginForm,\n      regForm,\n      rules,\n      validatePass2,\n      regRules,\n      toggleMode,\n      handleLogin,\n      handleRegister,\n      ref,\n      reactive,\n      getCurrentInstance,\n      get useRouter() {\n        return useRouter;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get User() {\n        return User;\n      },\n      get Lock() {\n        return Lock;\n      },\n      get CircleCheck() {\n        return CircleCheck;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","getCurrentInstance","useRouter","ElMessage","User","Lock","CircleCheck","proxy","router","isRegister","loading","loginFormRef","regFormRef","loginForm","username","password","regForm","confirmPass","rules","required","message","trigger","validatePass2","rule","value","callback","Error","regRules","validator","toggleMode","resetFields","handleLogin","validate","valid","res","$request","post","data","code","success","type","duration","localStorage","setItem","JSON","stringify","setTimeout","replace","error","msg","handleRegister"],"sources":["E:/xianyu/tnt/tnt_shopping_front/src/views/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-wrapper\">\r\n    <!-- 背景装饰字 (极淡) -->\r\n    <div class=\"bg-decoration\">TNT</div>\r\n\r\n    <div class=\"auth-box\">\r\n      <!-- 左侧：视觉冲击区 (改为黄底黑字，确保清晰度) -->\r\n      <div class=\"visual-side\">\r\n        <div class=\"brand-block\">\r\n          <!-- 巨型文字排版 -->\r\n          <h1 class=\"hero-text\">\r\n            <span class=\"line\">TEENS</span>\r\n            <span class=\"line\">IN</span>\r\n            <span class=\"line\">TIMES</span>\r\n          </h1>\r\n\r\n          <div class=\"black-divider\"></div>\r\n\r\n          <div class=\"sub-info\">\r\n            <p class=\"cn-slogan\">破天下 · 定风云</p>\r\n            <p class=\"en-slogan\">OFFICIAL STORE</p>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 装饰性条形码/元素 -->\r\n        <div class=\"barcode-decor\">\r\n          <span>|| | ||| | || |||</span>\r\n          <span class=\"code\">NO. 1123</span>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 右侧：功能表单区 (白底黑字) -->\r\n      <div class=\"form-side\">\r\n        <div class=\"header-group\">\r\n          <div class=\"logo-small\">TNT SPACE</div>\r\n          <h2>{{ isRegister ? 'MEMBER REGISTER' : 'USER LOGIN' }}</h2>\r\n        </div>\r\n\r\n        <!-- 登录表单 -->\r\n        <el-form\r\n            v-if=\"!isRegister\"\r\n            ref=\"loginFormRef\"\r\n            :model=\"loginForm\"\r\n            :rules=\"rules\"\r\n            class=\"heavy-form\"\r\n            size=\"large\"\r\n            hide-required-asterisk\r\n        >\r\n          <el-form-item prop=\"username\">\r\n            <div class=\"label-text\">USERNAME</div>\r\n            <el-input\r\n                v-model=\"loginForm.username\"\r\n                placeholder=\"请输入用户名\"\r\n                :prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <div class=\"label-text\">PASSWORD</div>\r\n            <el-input\r\n                v-model=\"loginForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"请输入密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <button class=\"solid-btn\" @click.prevent=\"handleLogin\">\r\n            <span v-if=\"!loading\">登录 / LOGIN</span>\r\n            <span v-else>LOADING...</span>\r\n          </button>\r\n        </el-form>\r\n\r\n        <!-- 注册表单 -->\r\n        <el-form\r\n            v-else\r\n            ref=\"regFormRef\"\r\n            :model=\"regForm\"\r\n            :rules=\"regRules\"\r\n            class=\"heavy-form\"\r\n            size=\"large\"\r\n            hide-required-asterisk\r\n        >\r\n          <el-form-item prop=\"username\">\r\n            <div class=\"label-text\">NEW USERNAME</div>\r\n            <el-input\r\n                v-model=\"regForm.username\"\r\n                placeholder=\"设置用户名\"\r\n                :prefix-icon=\"User\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <div class=\"label-text\">PASSWORD</div>\r\n            <el-input\r\n                v-model=\"regForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"设置密码\"\r\n                show-password\r\n                :prefix-icon=\"Lock\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item prop=\"confirmPass\">\r\n            <div class=\"label-text\">CONFIRM</div>\r\n            <el-input\r\n                v-model=\"regForm.confirmPass\"\r\n                type=\"password\"\r\n                placeholder=\"确认密码\"\r\n                show-password\r\n                :prefix-icon=\"CircleCheck\"\r\n            />\r\n          </el-form-item>\r\n\r\n          <button class=\"solid-btn outline\" @click.prevent=\"handleRegister\">\r\n            <span v-if=\"!loading\">注册 / REGISTER</span>\r\n            <span v-else>PROCESSING...</span>\r\n          </button>\r\n        </el-form>\r\n\r\n        <div class=\"footer-action\">\r\n          <span @click=\"toggleMode\">\r\n            {{ isRegister ? 'Back to Login' : 'Create an Account' }}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, getCurrentInstance } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { User, Lock, CircleCheck } from '@element-plus/icons-vue'\r\n\r\nconst { proxy } = getCurrentInstance()\r\nconst router = useRouter()\r\n\r\nconst isRegister = ref(false)\r\nconst loading = ref(false)\r\nconst loginFormRef = ref(null)\r\nconst regFormRef = ref(null)\r\n\r\nconst loginForm = reactive({ username: '', password: '' })\r\nconst regForm = reactive({ username: '', password: '', confirmPass: '' })\r\n\r\n// 校验规则\r\nconst rules = {\r\n  username: [{ required: true, message: 'Please input username', trigger: 'blur' }],\r\n  password: [{ required: true, message: 'Please input password', trigger: 'blur' }]\r\n}\r\n\r\nconst validatePass2 = (rule, value, callback) => {\r\n  if (value === '') callback(new Error('Please confirm password'))\r\n  else if (value !== regForm.password) callback(new Error('Password mismatch'))\r\n  else callback()\r\n}\r\n\r\nconst regRules = {\r\n  ...rules,\r\n  confirmPass: [{ validator: validatePass2, trigger: 'blur' }]\r\n}\r\n\r\n// 切换逻辑\r\nconst toggleMode = () => {\r\n  isRegister.value = !isRegister.value\r\n  loginFormRef.value?.resetFields()\r\n  regFormRef.value?.resetFields()\r\n}\r\n\r\n// 登录\r\nconst handleLogin = async () => {\r\n  if (!loginFormRef.value) return\r\n\r\n  await loginFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      loading.value = true\r\n      try {\r\n        const res = await proxy.$request.post('/user/login', loginForm)\r\n\r\n        if (res.data.code === '200') {\r\n          ElMessage.success({\r\n            message: '登录成功 / Welcome',\r\n            type: 'success',\r\n            duration: 1500\r\n          })\r\n          localStorage.setItem('user', JSON.stringify(res.data.data))\r\n\r\n          setTimeout(() => {\r\n            router.replace('/home')\r\n          }, 500)\r\n        } else {\r\n          ElMessage.error(res.data.msg || 'Login failed')\r\n        }\r\n      } catch (error) {\r\n        ElMessage.error('Server Error')\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n  })\r\n}\r\n\r\n// 注册\r\nconst handleRegister = async () => {\r\n  if (!regFormRef.value) return\r\n  await regFormRef.value.validate(async (valid) => {\r\n    if (valid) {\r\n      loading.value = true\r\n      try {\r\n        const res = await proxy.$request.post('/user/register', {\r\n          username: regForm.username,\r\n          password: regForm.password\r\n        })\r\n        if (res.data.code === '200') {\r\n          ElMessage.success('Register Success')\r\n          toggleMode()\r\n        } else {\r\n          ElMessage.error(res.data.msg)\r\n        }\r\n      } catch (error) {\r\n        ElMessage.error('Server Error')\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n  })\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 引入粗体无衬线字库 */\r\n@import url('https://fonts.googleapis.com/css2?family=Anton&family=Roboto:wght@400;700&display=swap');\r\n\r\n:root {\r\n  --page-bg: #FFFDF0;       /* 浅米黄大背景 */\r\n  --tnt-yellow: #FAD02C;    /* 核心应援色 (Popcorn Yellow) */\r\n  --tnt-black: #000000;     /* 纯黑 */\r\n  --tnt-white: #FFFFFF;\r\n}\r\n\r\n.page-wrapper {\r\n  min-height: 100vh;\r\n  background-color: var(--page-bg);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-family: 'Roboto', sans-serif;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n/* 背景大字装饰 */\r\n.bg-decoration {\r\n  position: absolute;\r\n  top: -10%;\r\n  left: -5%;\r\n  font-size: 30vw;\r\n  font-weight: 900;\r\n  color: rgba(250, 208, 44, 0.1); /* 极淡的黄色水印 */\r\n  font-family: 'Anton', sans-serif;\r\n  pointer-events: none;\r\n  z-index: 0;\r\n}\r\n\r\n.auth-box {\r\n  width: 900px;\r\n  min-height: 520px;\r\n  background: var(--tnt-white);\r\n  box-shadow: 0 30px 60px rgba(0,0,0,0.1);\r\n  display: flex;\r\n  z-index: 1;\r\n  border: 2px solid var(--tnt-black); /* 黑色描边，增加立体漫感 */\r\n}\r\n\r\n/* --- 左侧：视觉冲击区 (黄底黑字) --- */\r\n.visual-side {\r\n  width: 45%;\r\n  background-color: var(--tnt-yellow); /* 改为黄色背景 */\r\n  color: var(--tnt-black); /* 改为黑色文字 */\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  padding: 50px;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.brand-block {\r\n  z-index: 2;\r\n}\r\n\r\n.hero-text {\r\n  font-family: 'Anton', sans-serif; /* 使用极粗字体 */\r\n  font-size: 72px; /* 巨大字号 */\r\n  line-height: 0.9;\r\n  margin: 0;\r\n  text-transform: uppercase;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.hero-text .line {\r\n  display: block;\r\n  letter-spacing: -2px; /* 紧凑排列 */\r\n}\r\n\r\n.black-divider {\r\n  width: 80px;\r\n  height: 8px;\r\n  background-color: var(--tnt-black);\r\n 